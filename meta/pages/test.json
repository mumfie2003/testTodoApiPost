{
  "access": [
    "Everybody"
  ],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "My Todos",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Add",
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "filterValue": "",
                      "format": "int32",
                      "property": "id",
                      "title": "Id",
                      "type": "integer",
                      "width": "5rem"
                    },
                    {
                      "format": "",
                      "property": "title",
                      "title": "Title",
                      "type": "string"
                    },
                    {
                      "format": "int32",
                      "property": "order",
                      "title": "Order",
                      "type": "integer",
                      "width": "5rem"
                    },
                    {
                      "format": "",
                      "property": "completed",
                      "title": "Completed",
                      "type": "boolean",
                      "width": "5rem"
                    },
                    {
                      "format": "",
                      "property": "url",
                      "title": "Url",
                      "type": "string"
                    }
                  ],
                  "confirmDeleteCancelText": "Cancel",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Are you sure you want to delete this record?",
                  "confirmDeleteTitle": "Confirmation",
                  "containsText": "Contains",
                  "data": "${ListResult}",
                  "emptyText": "No records to display.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "loadData": [
                      {
                        "name": "ToDoBackend.List",
                        "then": [
                          {
                            "name": "ListResult",
                            "type": "set",
                            "value": "${result}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid1",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "type": "grid",
                  "value": ""
                }
              ],
              "name": "col0",
              "type": "col"
            }
          ],
          "name": "row0",
          "type": "row"
        },
        {
          "components": [
            {
              "buttonPosition": "bottom",
              "data": "${myToDo}",
              "events": {
                "submit": [
                  {
                    "error": [
                      {
                        "detail": "Error",
                        "severity": "error",
                        "summary": "Error",
                        "type": "notify"
                      }
                    ],
                    "name": "ToDoBackend.CreateToDo",
                    "parameters": [
                      {
                        "name": "ToDo",
                        "value": "${myToDo}"
                      }
                    ],
                    "then": [
                      {
                        "code": "this.grid1.load();\n${myToDo.title=\"\"};",
                        "error": [],
                        "type": "execute"
                      }
                    ],
                    "type": "invoke"
                  }
                ]
              },
              "fields": [
                {
                  "placeholder": "Give your ToDo a Title",
                  "property": "title",
                  "required": true,
                  "requiredText": "is required",
                  "title": "Title",
                  "type": "string"
                },
                {
                  "property": "completed",
                  "required": false,
                  "requiredText": "is required",
                  "title": "Completed",
                  "type": "boolean"
                }
              ],
              "name": "form0",
              "submitText": "Add Todo",
              "type": "form",
              "updateDataOnChange": true
            }
          ],
          "name": "fieldset0",
          "text": "Add New Todo",
          "type": "fieldset"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "buttonStyle": "danger",
                  "events": {
                    "click": [
                      {
                        "error": [
                          {
                            "detail": "Error",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ToDoBackend.ZapAllTodos",
                        "then": [
                          {
                            "code": "this.grid1.load();",
                            "type": "execute"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "name": "button0",
                  "text": "Delete All ToDos",
                  "type": "button"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row1",
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "myToDo",
        "type": "set",
        "value": "{\"title\":\"\",\"completed\":false}"
      },
      {
        "code": "this.grid1.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "Blank",
  "name": "test"
}